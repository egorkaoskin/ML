      "source": [
        "X_full = pd.concat([X_train, X_test], axis=0).reset_index(drop=True)\n",
        "y_full = pd.concat([y_train, y_test], axis=0).reset_index(drop=True)\n",
        "df = X_full.copy()"
      ],
      "metadata": {
        "id": "575shcnOzxhV"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df = X_full.copy()\n",
        "df['price'] = y_full.reset_index(drop=True)\n",
        "\n",
        "df['price_1'] = (\n",
        "    60000 +\n",
        "    12000 * df['interchangeable_lens'] +\n",
        "    1500 * np.sqrt(df['megapixels']) +\n",
        "    800 * (df['year_released'] - 2015) +\n",
        "    10000\n",
        ")\n",
        "\n",
        "df['price_0'] = (\n",
        "    50000 +\n",
        "    10000 * df['interchangeable_lens'] +\n",
        "    1200 * np.sqrt(df['megapixels']) +\n",
        "    700 * (df['year_released'] - 2015)\n",
        ")\n",
        "\n",
        "df['TE'] = df['price_1'] - df['price_0']\n",
        "CATE = df['TE']\n",
        "ATE = df['TE'].mean()\n",
        "\n",
        "df['diff_to_1'] = np.abs(df['price'] - df['price_1'])\n",
        "df['diff_to_0'] = np.abs(df['price'] - df['price_0'])\n",
        "df['agent_type'] = np.where(\n",
        "    (df['premium_brand'] == 1) & (df['diff_to_1'] < df['diff_to_0']),\n",
        "    'Complier',\n",
        "    np.where(\n",
        "        (df['premium_brand'] == 0) & (df['diff_to_0'] < df['diff_to_1']),\n",
        "        'Complier',\n",
        "        'Other'\n",
        "    )\n",
        ")\n",
        "\n",
        "ATE = df['TE'].mean()\n",
        "LATE = df[df['agent_type'] == 'Complier']['TE'].mean()\n",
        "ATE_naive = df[df['premium_brand'] == 1]['price'].mean() - df[df['premium_brand'] == 0]['price'].mean()\n",
        "\n",
        "results_df = pd.DataFrame({\n",
        "    'Метрика': ['ATE', 'LATE', 'ATE_naive'],\n",
        "    'Значение': [ATE, LATE, ATE_naive]\n",
        "})\n",
        "\n",
        "plt.figure(figsize=(18, 5))\n",
        "\n",
        "plt.subplot(1, 3, 1)\n",
        "plt.hist(df['TE'], bins=30, color='lightblue', edgecolor='black', density=True)\n",
        "plt.title(\"TE (True Effect)\")\n",
        "plt.xlabel(\"TE\")\n",
        "plt.ylabel(\"Плотность\")\n",
        "plt.grid(True)\n",
        "\n",
        "plt.subplot(1, 3, 2)\n",
        "plt.hist(df['TE'], bins=30, color='lightgreen', edgecolor='black', density=True)\n",
        "plt.title(\"CATE (Условный эффект)\")\n",
        "plt.xlabel(\"CATE\")\n",
        "plt.ylabel(\"Плотность\")\n",
        "plt.grid(True)\n",
        "\n",
        "plt.subplot(1, 3, 3)\n",
        "treated_prices = df[df['premium_brand'] == 1]['price']\n",
        "control_prices = df[df['premium_brand'] == 0]['price'].sample(\n",
        "    n=len(treated_prices), random_state=42, replace=True).values\n",
        "\n",
        "price_diff = treated_prices.values - control_prices\n",
        "plt.hist(price_diff, bins=30, color='salmon', edgecolor='black', density=True)\n",
        "plt.title(\"ATE naive: разность наблюдаемых цен\")\n",
        "plt.xlabel(\"Разность цен\")\n",
        "plt.ylabel(\"Плотность\")\n",
        "plt.grid(True)\n",
        "\n",
        "plt.tight_layout()\n",
        "df = X_full.copy()\n",
        "df['price'] = y_full.reset_index(drop=True)\n",
        "\n",
        "df['price_1'] = (\n",
        "    60000 +\n",
        "    12000 * df['interchangeable_lens'] +\n",
        "    1500 * np.sqrt(df['megapixels']) +\n",
        "    800 * (df['year_released'] - 2015) +\n",
        "    10000\n",
        ")\n",
        "\n",
        "df['price_0'] = (\n",
        "    50000 +\n",
        "    10000 * df['interchangeable_lens'] +\n",
        "    1200 * np.sqrt(df['megapixels']) +\n",
        "    700 * (df['year_released'] - 2015)\n",
        ")\n",
        "\n",
        "df['TE'] = df['price_1'] - df['price_0']\n",
        "CATE = df['TE']\n",
        "ATE = df['TE'].mean()\n",
        "\n",
        "df['diff_to_1'] = np.abs(df['price'] - df['price_1'])\n",
        "df['diff_to_0'] = np.abs(df['price'] - df['price_0'])\n",
        "df['agent_type'] = np.where(\n",
        "    (df['premium_brand'] == 1) & (df['diff_to_1'] < df['diff_to_0']),\n",
