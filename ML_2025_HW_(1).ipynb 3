      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import train_test_split, cross_val_score, RandomizedSearchCV, ParameterSampler\n",
        "from sklearn.linear_model import LinearRegression, Ridge\n",
        "from sklearn.neighbors import KNeighborsRegressor\n",
        "from sklearn.ensemble import RandomForestRegressor, GradientBoostingRegressor\n",
        "from sklearn.metrics import mean_squared_error\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "\n",
        "\n",
        "def tune_models_with_cv(data, target='price', cv=5, seed=42, n_iter=10):\n",
        "    evaluator = SimpleModelEvaluatorFixed(data, target, seed=seed)\n",
        "\n",
        "    models_with_params = {\n",
        "        'Linear Regression': (LinearRegression(), {}, {}),\n",
        "        'Ridge Regression': (Ridge(), {'alpha': [0.1, 1, 10]}, {'alpha': 1}),\n",
        "        'KNN': (KNeighborsRegressor(), {'n_neighbors': list(range(3, 16, 2))}, {'n_neighbors': 5}),\n",
        "        'Random Forest': (\n",
        "            RandomForestRegressor(random_state=seed),\n",
        "            {'n_estimators': [50, 100, 150], 'max_depth': [3, 5, 7, None], 'min_samples_split': [2, 4]},\n",
        "            {'n_estimators': 100, 'max_depth': 6, 'min_samples_split': 2}\n",
        "        ),\n",
        "        'Gradient Boosting': (\n",
        "            GradientBoostingRegressor(random_state=seed),\n",
        "            {'n_estimators': [100, 150], 'max_depth': [3, 4], 'learning_rate': [0.05, 0.1]},\n",
        "            {'n_estimators': 150, 'max_depth': 4, 'learning_rate': 0.1}\n",
        "        )\n",
        "    }\n",
        "\n",
        "    results = []\n",
        "\n",
        "    for name, (model, param_grid, default_params) in models_with_params.items():\n",
        "        model.set_params(**default_params)\n",
        "        model.fit(evaluator.X_train, evaluator.y_train)\n",
        "        y_test_pred_initial = model.predict(evaluator.X_test)\n",
        "        rmse_test_initial = mean_squared_error(evaluator.y_test, y_test_pred_initial)\n",
        "        rmse_cv_initial = evaluator._cv_rmse(model)\n",
        "\n",
        "        if param_grid:\n",
        "            search = RandomizedSearchCV(\n",
        "                model, param_distributions=param_grid,\n",
        "                scoring='neg_mean_squared_error', cv=cv,\n",
        "                n_iter=n_iter, random_state=seed\n",
        "            )\n",
        "            search.fit(evaluator.X_full, evaluator.y_full)\n",
        "            best_model = search.best_estimator_\n",
        "            best_params = search.best_params_\n",
        "            rmse_cv_tuned = np.sqrt(-search.best_score_)\n",
        "        else:\n",
        "            best_model = model\n",
        "            best_params = default_params\n",
        "            rmse_cv_tuned = rmse_cv_initial\n",
        "\n",
        "        y_test_pred_tuned = best_model.predict(evaluator.X_test)\n",
        "        rmse_test_tuned = mean_squared_error(evaluator.y_test, y_test_pred_tuned)\n",
        "\n",
        "        results.append({\n",
        "            'Model': name,\n",
        "            'Initial Params': default_params,\n",
        "            'Tuned Params': best_params,\n",
        "            'CV RMSE (Initial)': rmse_cv_initial,\n",
        "            'CV RMSE (Tuned)': rmse_cv_tuned,\n",
        "            'Test RMSE (Initial)': rmse_test_initial,\n",
        "            'Test RMSE (Tuned)': rmse_test_tuned\n",
        "        })\n",
        "\n",
        "    return pd.DataFrame(results)\n",
        "\n",
        "\n",
        "def tune_random_forest_oob(data, target='price', seed=42, n_iter=10):\n",
        "    evaluator = SimpleModelEvaluatorFixed(data, target, seed=seed)\n",
        "\n",
        "    param_distributions = {\n",
        "        'n_estimators': [50, 100, 150],\n",
        "        'max_depth': [3, 5, 7, None],\n",
        "        'min_samples_split': [2, 4]\n",
        "    }\n",
        "\n",
        "    sampled_params = list(ParameterSampler(param_distributions, n_iter=n_iter, random_state=seed))\n",
        "    best_model = None\n",
        "    best_params = None\n",
        "    best_oob_score = -np.inf\n",
        "\n",
        "    for params in sampled_params:\n",
        "        model = RandomForestRegressor(oob_score=True, random_state=seed, **params)\n",
        "        model.fit(evaluator.X_full, evaluator.y_full)\n",
        "\n",
        "        if model.oob_score_ > best_oob_score:\n",
        "            best_oob_score = model.oob_score_\n",
        "            best_model = model\n",
        "            best_params = params\n",
        "\n",
        "    y_test_pred = best_model.predict(evaluator.X_test)\n",
        "    test_rmse = mean_squared_error(evaluator.y_test, y_test_pred)\n",
        "\n",
        "    return {\n",
        "        'Best Params (RandomForest)': best_params,\n",
        "        'Best OOB Score': best_oob_score,\n",
        "        'Test RMSE (OOB Tuned)': test_rmse\n",
        "    }\n",
        "\n",
        "\n",
        "cv_results = tune_models_with_cv(dfreg)\n",
        "rf_oob_result = tune_random_forest_oob(dfreg)\n",
        "\n",
        "\n",
        "cv_gb_result = cv_results[cv_results['Model'] == 'Gradient Boosting'].iloc[0]\n",
        "comparison_df = pd.DataFrame([\n",
        "    {\n",
        "        'Model': 'Gradient Boosting (CV)',\n",
        "        'Tuning Method': 'Cross-Validation',\n",
        "        'Best Params': str(cv_gb_result['Tuned Params']),\n",
        "        'Validation Metric': round(cv_gb_result['CV RMSE (Tuned)'], 2),\n",
        "        'Test RMSE': round(cv_gb_result['Test RMSE (Tuned)'], 2)\n",
        "    },\n",
        "    {\n",
        "        'Model': 'Random Forest (OOB)',\n",
        "        'Tuning Method': 'Out-of-Bag',\n",
        "        'Best Params': str(rf_oob_result['Best Params (RandomForest)']),\n",
        "        'Validation Metric': f\"OOB R² = {rf_oob_result['Best OOB Score']:.3f}\",\n",
        "        'Test RMSE': round(rf_oob_result['Test RMSE (OOB Tuned)'], 2)\n",
        "    }\n",
        "])\n",
        "\n",
        "\n",
        "for model_name in ['Linear Regression', 'Ridge Regression', 'KNN']:\n",
        "    model_row = cv_results[cv_results['Model'] == model_name].iloc[0]\n",
        "    comparison_df = pd.concat([\n",
        "        comparison_df,\n",
        "        pd.DataFrame([{\n",
        "            'Model': model_name,\n",
        "            'Tuning Method': 'Cross-Validation',\n",
        "            'Best Params': str(model_row['Tuned Params']),\n",
        "            'Validation Metric': round(model_row['CV RMSE (Tuned)'], 2),\n",
        "            'Test RMSE': round(model_row['Test RMSE (Tuned)'], 2)\n",
        "        }])\n",
        "    ], ignore_index=True)\n",
        "print(comparison_df['Best Params'])\n",
        "\n",
        "fig, ax = plt.subplots(figsize=(14, 4 + 0.5 * len(comparison_df)))\n",
        "ax.axis('tight')\n",
        "ax.axis('off')\n",
        "table = ax.table(cellText=comparison_df.values,\n",
        "                 colLabels=comparison_df.columns,\n",
        "                 cellLoc='center',\n",
        "                 loc='center')\n",
        "table.auto_set_font_size(False)\n",
        "table.set_fontsize(10)\n",
        "table.scale(1, 1.5)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
