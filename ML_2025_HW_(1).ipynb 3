    {
      "cell_type": "markdown",
      "source": [
        "##Задание 5.4"
      ],
      "metadata": {
        "id": "i1bbzUlUjt5-"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "from sklearn.linear_model import LinearRegression, LogisticRegression\n",
        "from sklearn.ensemble import GradientBoostingRegressor\n",
        "from sklearn.model_selection import train_test_split\n",
        "\n",
        "X_full = pd.concat([X_train, X_test], axis=0).reset_index(drop=True)\n",
        "y_full = pd.concat([y_train, y_test], axis=0).reset_index(drop=True)\n",
        "\n",
        "df = X_full.copy()\n",
        "df['price'] = y_full\n",
        "df['D'] = df['premium_brand']\n",
        "\n",
        "ols_model = LinearRegression()\n",
        "ols_model.fit(df[['D']], df['price'])\n",
        "ate_ols = ols_model.coef_[0]\n",
        "\n",
        "ate_cme = df[df['D'] == 1]['price'].mean() - df[df['D'] == 0]['price'].mean()\n",
        "\n",
        "X_prop = df.drop(columns=['price', 'D'])\n",
        "prop_model = LogisticRegression()\n",
        "prop_model.fit(X_prop, df['D'])\n",
        "pscore = prop_model.predict_proba(X_prop)[:, 1]\n",
        "pscore = np.clip(pscore, 1e-3, 1 - 1e-3)\n",
        "\n",
        "ate_ipw = np.mean(df['D'] * df['price'] / pscore - (1 - df['D']) * df['price'] / (1 - pscore))\n",
        "\n",
        "mu_model_1 = GradientBoostingRegressor().fit(df[df['D'] == 1].drop(columns=['price', 'D']), df[df['D'] == 1]['price'])\n",
        "mu_model_0 = GradientBoostingRegressor().fit(df[df['D'] == 0].drop(columns=['price', 'D']), df[df['D'] == 0]['price'])\n",
        "mu1_pred = mu_model_1.predict(X_prop)\n",
        "mu0_pred = mu_model_0.predict(X_prop)\n",
        "\n",
        "dr = ((df['D'] * (df['price'] - mu1_pred)) / pscore -\n",
        "      ((1 - df['D']) * (df['price'] - mu0_pred)) / (1 - pscore) +\n",
        "      mu1_pred - mu0_pred)\n",
        "ate_dr = dr.mean()\n",
        "\n",
        "from sklearn.base import clone\n",
        "\n",
        "X = df.drop(columns=['price', 'D'])\n",
        "Y = df['price']\n",
        "D = df['D']\n",
        "\n",
        "X_dml_train, X_dml_val, D_train, D_val, Y_train, Y_val = train_test_split(\n",
        "    X, D, Y, test_size=0.5, random_state=42\n",
        ")\n",
        "\n",
        "model_Y = GradientBoostingRegressor()\n",
        "model_D = GradientBoostingRegressor()\n",
        "\n",
        "m_Y = clone(model_Y).fit(X_dml_train, Y_train)\n",
        "m_D = clone(model_D).fit(X_dml_train, D_train)\n",
        "\n",
        "Y_hat = m_Y.predict(X_dml_val)\n",
        "D_hat = m_D.predict(X_dml_val)\n",
        "\n",
        "res_Y = Y_val - Y_hat\n",
        "res_D = D_val - D_hat\n",
        "\n",
        "dml_final_model = LinearRegression().fit(res_D.values.reshape(-1, 1), res_Y)\n",
        "ate_dml = dml_final_model.coef_[0]\n",
        "\n",
        "\n",
        "results = pd.DataFrame({\n",
        "    \"Метод\": [\n",
        "        \"OLS (МНК)\",\n",
        "        \"CME (условные средние)\",\n",
        "        \"IPW (взвешивание)\",\n",
        "        \"DR (двойная устойчивость)\",\n",
        "        \"DML (двойное машинное обучение)\"\n",
        "    ],\n",
        "    \"ATE\": [ate_ols, ate_cme, ate_ipw, ate_dr, ate_dml]\n",
        "})\n",
        "\n",
        "print(results)\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "e9Q1J5BKjw10",
        "outputId": "5114fd88-7fe3-4546-ee9d-82eca484be76"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "                             Метод           ATE\n",
            "0                        OLS (МНК)  3.667571e+04\n",
            "1           CME (условные средние)  3.667571e+04\n",
            "2                IPW (взвешивание)  7.017154e+04\n",
            "3        DR (двойная устойчивость)  3.594337e+04\n",
            "4  DML (двойное машинное обучение)  1.318319e+08\n"
          ]
        }
