    {
      "cell_type": "code",
      "source": [
        "dfreg = df[['megapixels', 'year_released', 'interchangeable_lens', 'used_in_cinema']]"
      ],
      "metadata": {
        "id": "6OEdlDBSQSpz"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "##Задание 4.2"
      ],
      "metadata": {
        "id": "kd61bPdTQzsA"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.linear_model import LinearRegression, Ridge, LogisticRegression\n",
        "from sklearn.neighbors import KNeighborsRegressor, KNeighborsClassifier\n",
        "from sklearn.ensemble import RandomForestRegressor, RandomForestClassifier, GradientBoostingClassifier, GradientBoostingRegressor\n",
        "from sklearn.metrics import roc_curve, auc, mean_squared_error, mean_absolute_percentage_error, make_scorer, confusion_matrix, roc_auc_score, f1_score, recall_score, accuracy_score\n",
        "\n",
        "\n",
        "dfreg = df[['price', 'megapixels', 'year_released', 'interchangeable_lens', 'used_in_cinema']]\n",
        "class SimpleModelEvaluatorFixed:\n",
        "    def __init__(self, data, target, test_size=0.25, val_size=0.1, seed=42):\n",
        "        self.data = data\n",
        "        self.target = target\n",
        "        self.test_size = test_size\n",
        "        self.val_size = val_size\n",
        "        self.seed = seed\n",
        "        self._prepare_splits()\n",
        "\n",
        "    def _prepare_splits(self):\n",
        "        X = self.data.drop(columns=[self.target])\n",
        "        y = self.data[self.target]\n",
        "\n",
        "        X_temp, self.X_test, y_temp, self.y_test = train_test_split(\n",
        "            X, y, test_size=self.test_size, random_state=self.seed\n",
        "        )\n",
        "        val_share = self.val_size / (1 - self.test_size)\n",
        "        self.X_train, self.X_val, self.y_train, self.y_val = train_test_split(\n",
        "            X_temp, y_temp, test_size=val_share, random_state=42\n",
        "        )\n",
        "\n",
        "        self.X_full = pd.concat([self.X_train, self.X_val])\n",
        "        self.y_full = pd.concat([self.y_train, self.y_val])\n",
        "\n",
        "    def _rmse(self, actual, predicted):\n",
        "        return mean_squared_error(actual, predicted)\n",
        "\n",
        "    def _mape(self, actual, predicted):\n",
        "        return mean_absolute_percentage_error(actual, predicted)\n",
        "\n",
        "    def _cv_rmse(self, model, cv=5):\n",
        "        scores = cross_val_score(model, self.X_full, self.y_full, scoring='neg_mean_squared_error', cv=cv)\n",
        "        return np.mean(np.sqrt(-scores))\n",
        "\n",
        "    def _cv_mape(self, model, cv=5):\n",
        "        scorer = make_scorer(mean_absolute_percentage_error, greater_is_better=False)\n",
        "        scores = cross_val_score(model, self.X_full, self.y_full, scoring=scorer, cv=cv)\n",
        "        return -scores.mean()\n",
        "\n",
        "    def evaluate(self):\n",
        "        models = {\n",
        "            'Linear Regression': LinearRegression(),\n",
        "            'Ridge Regression': Ridge(alpha=1),\n",
        "            'KNN': KNeighborsRegressor(n_neighbors=5),\n",
        "            'Random Forest': RandomForestRegressor(n_estimators=100, max_depth=6, random_state=42),\n",
        "            'Gradient Boosting': GradientBoostingRegressor(n_estimators=150, learning_rate=0.1, max_depth=4, random_state=42)\n",
        "        }\n",
        "\n",
        "        summary = []\n",
        "        for name, model in models.items():\n",
        "            model.fit(self.X_train, self.y_train)\n",
        "            y_train_pred = model.predict(self.X_train)\n",
        "            y_test_pred = model.predict(self.X_test)\n",
        "\n",
        "            summary.append({\n",
        "                'Model': name,\n",
        "                'Train RMSE': self._rmse(self.y_train, y_train_pred),\n",
        "                'Test RMSE': self._rmse(self.y_test, y_test_pred),\n",
        "                'CV RMSE': self._cv_rmse(model),\n",
        "                'Train MAPE': self._mape(self.y_train, y_train_pred),\n",
        "                'Test MAPE': self._mape(self.y_test, y_test_pred),\n",
        "                'CV MAPE': self._cv_mape(model)\n",
        "            })\n",
        "\n",
        "        return pd.DataFrame(summary)\n",
        "\n",
        "\n",
        "evaluator_fixed = SimpleModelEvaluatorFixed(dfreg, target='price')\n",
        "evaluation_results_fixed = evaluator_fixed.evaluate()\n",
        "evaluation_results_fixed"
      ],
      "metadata": {
        "id": "hcReITaXQ2sT",
        "outputId": "2d57a70d-a2b0-4218-b8e5-6b1c1717bcfe",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 226
        }
