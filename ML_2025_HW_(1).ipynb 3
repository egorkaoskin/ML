        "##Задание 5.7"
      ],
      "metadata": {
        "id": "AQO3fHMsNSb8"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "df['price_1'] = (\n",
        "    60000 +\n",
        "    12000 * df['interchangeable_lens'] +\n",
        "    1500 * np.sqrt(df['megapixels']) +\n",
        "    800 * (df['year_released'] - 2015) +\n",
        "    10000\n",
        ")\n",
        "\n",
        "df['price_0'] = (\n",
        "    50000 +\n",
        "    10000 * df['interchangeable_lens'] +\n",
        "    1200 * np.sqrt(df['megapixels']) +\n",
        "    700 * (df['year_released'] - 2015)\n",
        ")\n",
        "\n",
        "df['true_cate'] = df['price_1'] - df['price_0']\n",
        "\n",
        "\n",
        "df['true_cate'] = df['price_1'] - df['price_0']\n",
        "from sklearn.metrics import mean_squared_error\n",
        "\n",
        "models_preds = {\n",
        "    'S-learner': cate_s,\n",
        "    'T-learner': cate_t,\n",
        "    'X-learner': cate_x\n",
        "}\n",
        "\n",
        "true_cate = df['true_cate'].values\n",
        "results_true = []\n",
        "\n",
        "for name, preds in models_preds.items():\n",
        "    pehe = np.sqrt(mean_squared_error(true_cate, preds))\n",
        "    results_true.append({'Model': name, 'PEHE (True CATE)': pehe})\n",
        "\n",
        "results_true_df = pd.DataFrame(results_true)\n",
        "print(results_true_df)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "s7r21HO-K0OE",
        "outputId": "1a26ec4b-3790-4ced-e918-bfcd506ce638"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
