      "cell_type": "code",
      "source": [
        "from sklearn.metrics import mean_squared_error\n",
        "\n",
        "df['treated_pred'] = df['D'] * df['price_1'] + (1 - df['D']) * df['price_0']\n",
        "\n",
        "results_pred_acc = []\n",
        "\n",
        "for name, preds in models_preds.items():\n",
        "    price_pred = df['price_0'] + preds * df['D']\n",
        "    mse = mean_squared_error(df['price'], price_pred)\n",
        "    results_pred_acc.append({'Model': name, 'MSE (Price pred)': mse})\n",
        "\n",
        "results_pred_df = pd.DataFrame(results_pred_acc)\n",
        "print(results_pred_df)\n"
      ],
