    {
      "cell_type": "code",
      "source": [
        "from numpy.linalg import det, inv\n",
        "\n",
        "class LinearRegressionWithMCD:\n",
        "    def __init__(self, h_fraction=0.75, outlier_fraction=0.05):\n",
        "        self.h_fraction = h_fraction\n",
        "        self.outlier_fraction = outlier_fraction\n",
        "        self.model = LinearRegression()\n",
        "\n",
        "    def _mahalanobis_distances(self, X, mean_vec, cov_matrix):\n",
        "        inv_cov = inv(cov_matrix)\n",
        "        diffs = X - mean_vec\n",
        "        dists = np.sum(diffs @ inv_cov * diffs, axis=1)\n",
        "        return dists\n",
        "\n",
        "    def _compute_cov_det(self, X_subset):\n",
        "        cov = np.cov(X_subset, rowvar=False)\n",
        "        return det(cov) if det(cov) > 0 else np.inf\n",
        "\n",
        "    def _find_best_subset(self, X):\n",
        "        n_samples = X.shape[0]\n",
        "        h_size = int(self.h_fraction * n_samples)\n",
        "        min_det = np.inf\n",
        "        best_subset = None\n",
        "\n",
        "        for _ in range(50):\n",
        "            indices = np.random.choice(n_samples, h_size, replace=False)\n",
        "            subset = X[indices]\n",
        "            subset_det = self._compute_cov_det(subset)\n",
        "            if subset_det < min_det:\n",
        "                min_det = subset_det\n",
        "                best_subset = subset\n",
        "\n",
        "        return best_subset\n",
        "\n",
        "    def fit(self, X, y):\n",
        "        if isinstance(X, pd.DataFrame): X = X.values\n",
        "        if isinstance(y, pd.Series): y = y.values.reshape(-1, 1)\n",
        "        data = np.hstack((X, y))\n",
        "\n",
        "        core_subset = self._find_best_subset(data)\n",
        "        mean_vec = np.mean(core_subset, axis=0)\n",
        "        cov_matrix = np.cov(core_subset, rowvar=False)\n",
        "\n",
        "        dists = self._mahalanobis_distances(data, mean_vec, cov_matrix)\n",
        "        threshold = np.percentile(dists, (1 - self.outlier_fraction) * 100)\n",
        "        mask = dists < threshold\n",
        "        cleaned = data[mask]\n",
        "\n",
        "        self.model.fit(cleaned[:, :-1], cleaned[:, -1])\n",
        "        self.mean_vec = mean_vec\n",
        "        self.cov_matrix = cov_matrix\n",
        "\n",
        "    def predict(self, X):\n",
        "        if isinstance(X, pd.DataFrame):\n",
        "            X = X.values\n",
        "        return self.model.predict(X)\n",
        "\n",
        "    def get_model_coefficients(self):\n",
        "        return self.model.coef_, self.model.intercept_\n",
        "\n",
        "mcd_model = LinearRegressionWithMCD(h_fraction=0.75, outlier_fraction=0.05)\n",
        "mcd_model.fit(X_train, y_train)\n",
        "y_pred_mcd = mcd_model.predict(X_test)\n",
        "mcd_rmse = mean_squared_error(y_test, y_pred_mcd)\n",
        "mcd_rmse"
      ],
      "metadata": {
        "id": "Xjn_aY7PnUWb",
        "outputId": "53b9b2a1-f801-4871-fcfa-8ba5ccc6e9ff",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "58570031.72313713"
            ]
          },
          "metadata": {},
          "execution_count": 16
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "mcd_model = LinearRegressionWithMCD(h_fraction=0.75, outlier_fraction=0.05)\n",
        "mcd_model.fit(X_train, y_train)\n",
        "\n",
        "coefficients, intercept = mcd_model.get_model_coefficients()\n",
        "print(f\"Коэффициенты: {coefficients}\")\n",
        "print(f\"Свободный член: {intercept}\")\n",
        "\n",
        "pred_train = mcd_model.predict(X_train)\n",
        "pred_test = mcd_model.predict(X_test)\n",
        "\n",
        "rmse_train = mean_squared_error(y_train, pred_train)\n",
        "rmse_test = mean_squared_error(y_test, pred_test)\n",
        "\n",
        "mape_train = mean_absolute_percentage_error(y_train, pred_train)\n",
        "mape_test = mean_absolute_percentage_error(y_test, pred_test)\n",
        "\n",
        "print(f\"\\nRMSE (Train): {rmse_train:.3f}\")\n",
        "print(f\"RMSE (Test): {rmse_test:.3f}\")\n",
        "print(f\"MAPE (Train): {mape_train:.2%}\")\n",
