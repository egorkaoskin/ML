      "cell_type": "code",
      "source": [
        "pseudo_Y = np.where(\n",
        "    df['D'] == 1,\n",
        "    df['price'] - df['price_0'],\n",
        "    df['price_1'] - df['price']\n",
        ")\n",
        "\n",
        "for name, preds in models_preds.items():\n",
